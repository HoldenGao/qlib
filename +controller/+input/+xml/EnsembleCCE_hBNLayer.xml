<?xml version="1.0" encoding="utf-8"?>
<config>
   <Name>
       <SolutionName type="char">BenzeneDynamics</SolutionName>
   </Name>
   <Condition>
      <MagneticField type="eval">1e-4*[0 0 100]</MagneticField>
   </Condition>
   <CentralSpinSetting>
       <Name type="char">NV</Name>
       <CentralSpinStates type="eval">[1,2]</CentralSpinState>
       <SetSpin type="double">0</SetSpin>
       <Oritation type="eval">[1,1,1]</Oritation>
       <Isotope type="char">14N</Isotope>
       <Coordinate type="eval">[0,0,0]</Coordinate>
   </CentralSpinSetting>
   <Dynamics>
      <NTime type="double">100</NTime>
      <TMax type="eval">3e-4</TMax>
      <NPulse type="double">1</NPulse>
      
   </Dynamics>
   <InitialState>
      <DensityMatrix type="char">1.0 * p(2)_1 * p(1)_2 * p(1)_3 * p(1)_4 * p(1)_5 * p(1)_6</DensityMatrix>
   </InitialState>
   <Interaction>
      <IsSecular type="double">1</IsSecular>
   </Interaction>
   <SpinCollection>
      <FileName type="char">+xyz/BenzeneProtonZPlane.xyz</FileName>
      <Source type="char">File</Source>
   </SpinCollection>
   <BathSpinsSetting>
       <SetSpin type="double">1</SetSpin>
       <SpeciesNumber type="double">4</SpeciesNumber>
       <Name type="eval">["10B"; "11B"; "14N"; "15N"]</Name>
       <ZFS type="eval">[2*pi*1.48e5;2*pi*7.267e5;2*pi*1.05e5;0]</ZFS>
       <eta type="char">default</eta>
       <principle_axis type="char">default</principle_axis>       
   </BathSpinsSetting>
</config>